<div id="multipleimages">
<% for collection in @collections %>
	<div class="thumb fluid">
		<h3><%= link_to collection.title, collection %></h3>
		<div id="ajax">
		<%= link_to (image_tag collection.albums.find(:first).photos.first.file.collection.url), collection_path(collection), :class => "get" %>
		</div>
	</div>
<% end %>
</div>